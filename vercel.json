{
  "buildCommand": "vite build",
  "outputDirectory": "dist/client",
  "functions": {
    "api/health.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/health",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
}